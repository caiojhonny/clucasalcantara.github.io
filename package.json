{
  "name": "clucasalcantara.github.io",
  "description": "My personal website written in react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.9",
    "@emotion/styled": "^10.0.9",
    "cross-env": "^5.2.0",
    "emotion-theming": "^10.0.9",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "facepaint": "^1.2.1",
    "html-react-parser": "^0.6.3",
    "jest-enzyme": "^7.0.1",
    "prop-types": "^15.6.2",
    "react": "16.8.0-alpha.1",
    "react-dom": "16.8.0-alpha.1",
    "react-icons": "^3.5.0",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "react-test-renderer": "^16.7.0",
    "react-toggle": "^4.0.2",
    "unsplash-js": "^5.0.0",
    "webpack": "4.19.1"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "eslint src/ --fix",
      "git add"
    ]
  },
  "scripts": {
    "start": "cross-env NODE_PATH=src react-scripts start",
    "build": "cross-env NODE_PATH=src react-scripts build",
    "test": "cross-env NODE_PATH=src react-scripts test",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test:ci": "cross-env NODE_PATH=src react-scripts test --coverage",
    "eject": "react-scripts eject",
    "docz:dev": "docz dev",
    "docz:build": "docz build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "docz": "^0.13.7",
    "docz-plugin-css": "^0.11.0",
    "docz-theme-default": "^0.13.7",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.1",
    "pretty-quick": "^1.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cross-env NODE_ENV=production lint-staged",
      "pre-push": "cross-env NODE_ENV=production lint-staged && yarn test:ci"
    }
  }
}
